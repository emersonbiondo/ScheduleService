{
  "Schedule": {
    "ServiceName": "Schedule Service",
    "UseCrontab": true,
    "Crontab": "*/10 * * * * *"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "File": {
      "Path": "Schedule.log",
      "Append": true,
      "MinLevel": "Warning",
      "FileSizeLimitBytes": 1048576,
      "MaxRollingFiles": 5
    }
  },

  "ItemCommands": [
    {
      "OrderId": 1,
      "Type": "Console",
      "Delay": 1,
      "Parameter1": "",
      "Parameter2": "",
      "UseShellExecute": true,
      "CreateNoWindow": true,
      "WaitForExit": false
    },
    {
      "OrderId": 2,
      "Type": "HttpGet",
      "Delay": 1,
      "Parameter1": "",
      "Parameter2": ""
    },
    {
      "OrderId": 3,
      "Type": "SqlServerCommand",
      "Delay": 1,
      "Parameter1": "",
      "Parameter2": ""
    }
  ]
}
